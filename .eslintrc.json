{
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": 2021
	},
	"extends": ["plugin:v3xlabs/recommended"],
	"ignorePatterns": ["!**/*"],
	"plugins": ["v3xlabs"],
	"env": {
		"node": true
	},
	"rules": {
		"quotes": "off",
		"unicorn/prefer-at": "off",
		"unicorn/prefer-node-protocol": "off",
		"unused-imports/no-unused-imports": "off",
		"unused-imports/no-unused-vars": "off"
	}
}
